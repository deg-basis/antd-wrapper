{
  "name": "crux",
  "version": "1.0.0",
  "description": "Components for Rosette User eXperience",
  "main": "./dist/index.js",
  "scripts": {
    "build": "run-s clean tsc copy-css copy-assets",
    "tsc": "tsc",
    "copy-css": "cpx src/**/*.css dist",
    "copy-assets": "ncp src/assets dist/assets",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "lint": "run-p lint:*",
    "lint:ts": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "lint:style": "stylelint --fix \"src/**/*.css\"",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "clean": "rimraf ./dist"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "react": "^16.13.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.23",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "cpx": "^1.5.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^5.1.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-docgen": "^5.3.0",
    "react-docgen-typescript": "^1.16.2",
    "react-dom": "^16.13.0",
    "react-styleguidist": "^11.0.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0"
  }
}
